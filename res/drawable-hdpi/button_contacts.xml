<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#FF9215" />
            <stroke
                android:width="1dp"
                android:color="#7F4D11" />
            <corners
                android:radius="1dp" />

        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#FF9215"
                android:endColor="#7F4D11"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#7F4D11" />
            <corners
                android:radius="1dp" />

        </shape>
    </item>
</selector>